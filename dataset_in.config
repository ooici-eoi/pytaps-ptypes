[ncom]
in_path='test_data/ncom.nc'
out_path='test_data/ncom.h5m'
T0_var='time'
S0_vars={'x_var':'lon', 'y_var':'lat','z_var':None,'flags':['centroid']}
param_map={'S2':['salinity','surf_el','water_temp','water_u','water_v']} #2d params

[hfr]
in_path='test_data/hfr.nc'
out_path='test_data/hfr.h5m'
T0_var='time'
S0_vars={'x_var':'lon', 'y_var':'lat','z_var':None,'flags':['centroid']}
param_map={'S2':['DOPx','DOPy','u','v']}

[roms]
in_path='test_data/roms.nc'
out_path='test_data/roms.h5m'
T0_var='ocean_time'
S0_vars={'x_var':'lon_psi', 'y_var':'lat_psi','z_var':None,'flags':[]}
S1X_vars={'x_var':'lon_v','y_var':'lat_v','z_var':None,'flags':[]}
S1Y_vars={'x_var':'lon_u','y_var':'lat_u','z_var':None,'flags':[]}
param_map={'S0':['shflux','swrad','zeta','salt','temp','AKs','AKt','AKv', 'w'],'S1Y':['DU_avg1','DU_avg2','sustr','ubar','u'],'S1X':['DV_avg1','DV_avg2','svstr','vbar','v']}