[ncom]
in_path='test_data/ncom.nc'
out_path='test_data/ncom.h5m'
T0_var='time'
S0_vars={'x_var':'lon', 'y_var':'lat','z_var':None,'flags':['centroid']}
param_map={'S2':{'salinity':(0,slice(None),slice(None)),'surf_el':(slice(None),slice(None)),'water_temp':(0,slice(None),slice(None)),'water_u':(0,slice(None),slice(None)),'water_v':(0,slice(None),slice(None))}} #2d params

[hfr]
