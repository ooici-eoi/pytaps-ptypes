out_path='test_data/hfr.h5m'
T0_var='time'
S0_vars={'x_var':'lon', 'y_var':'lat','z_var':None,'flags':['centroid']}
param_map={'S2':{'DOPx':(slice(None),slice(None)),'DOPy':(slice(None),slice(None)),'u':(slice(None),slice(None)),'v':(slice(None),slice(None))}}

[roms]
in_path='test_data/roms.nc'
